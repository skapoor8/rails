<h1> All Users </h1>


<ul class="users">
	<% @users.each do |user| %>
		<li>
			<a href="#{link_to user}">
				<span> 
					<%= user.first_name %>
					<%= user.last_name %>
					<%= user.email %>
					<%= user.phone_number %>
					<% if current_user.admin? && !current_user?(user) %>
					| <%= link_to "delete", user, method: :delete, data: {confirm: "Are you sure?"} %>
					<% end %>
				</span>
			</a>
		</li>
	<% end %>
</ul>

